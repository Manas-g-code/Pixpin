<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@6.4.2/css/fontawesome.min.css" integrity="sha384-BY+fdrpOd3gfeRvTSMT+VUZmA728cfF9Z2G42xpaRkUGu2i3DyzpTURDo5A6CaLK" crossorigin="anonymous"> -->
  <link href="https://cdn.jsdelivr.net/npm/remixicon@4.2.0/fonts/remixicon.css" rel="stylesheet" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <link rel="stylesheet" href="/stylesheets/styleshowpost.css">
</head>

<body>
  <div class="navm">
    <div class="box">
      <h3><span class="pix"></span></h3>
    </div>
    <div class="try"></div>

    <a class="p" href="/profile">Profile</a>
    <a class="f" href="/feed">Feed</a>
    <a class="addnewPost" href="/add">Create</a>
    <a class="l" href="/logout">Logout</a>
  </div>
  <div class="cards">

    <% user.posts.forEach(function(elem){ %>
      <div class="c1">
        <div class="card">
          <form action="/delete/<%= elem._id %>" method="post">
          <button class="bin">
            <i class="ri-delete-bin-6-line"></i>
          </button>
        </form>
          <img class="cardimg" src="/images/uploads/<%= elem.image %>" alt="">
        </div>
        <h5 class="h5card"><%= elem.imageTitle %></h5>
      </div>
      <% }) %>
  </div>
  <script src="https://unpkg.com/typed.js@2.1.0/dist/typed.umd.js"></script>
<script>
   var typed=new Typed('.pix',{
      strings : ["PIXPIN","pixpin"],
      typeSpeed : 150,
      backSpeed : 150,
      loop : true
    })
</script>
</body>

</html>